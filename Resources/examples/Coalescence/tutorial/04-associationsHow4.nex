BEGIN TAXA;
	TITLE Species;
	DIMENSIONS NTAX=3;
	TAXLABELS
		speciesA speciesB speciesC 
	;
	BLOCKID WM1148e51203131;

END;



BEGIN TAXA;
	TITLE Genes;
	DIMENSIONS NTAX=5;
	TAXLABELS
		geneA1 geneA2 geneB geneC1 geneC2 
	;
	BLOCKID WM1148e51204232;

END;


BEGIN TaxaAssociation;
TITLE how_genes_fit_into_species;
TAXA Species ,  Genes;
ASSOCIATES 

	speciesA /  geneA1 geneA2 , 
	speciesB /  geneB , 
	speciesC /  geneC1 geneC2

;

END;


BEGIN TREES;
	Title 'Trees block 2 from 04-associationsHow4.nex';
	ID 0195bf551624136;
	LINK Taxa = Genes;
	TRANSLATE
[0] 		1 geneA1,
[1] 		2 geneA2,
[2] 		3 geneB,
[3] 		4 geneC1,
[4] 		5 geneC2;
	TREE gene_tree = ((1,2),(3,(4,5)));

END;


BEGIN TREES;
	Title 'Trees block 2 from 04-associationsHow4.nex';
	ID 0195bf551624137;
	LINK Taxa = Species;
	TRANSLATE
[0] 		1 speciesA,
[1] 		2 speciesB,
[2] 		3 speciesC;
	TREE species_tree = ((1,2),3);

END;


